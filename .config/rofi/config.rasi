configuration {
  modes: "window,drun,run,ssh";
  font: "Roboto Mono Medium 11.0";
  location: 0;
  yoffset: 0;
  xoffset: 0;
  show-icons: true;
  terminal: "alacritty";
  timeout {
      action: "kb-cancel";
      delay:  0;
  }
  filebrowser {
      directories-first: true;
      sorting-method: "name";
  }
}
@theme "./theme.rasi"
